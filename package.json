{
  "name": "cms-wordpress",
  "version": "1.0.0",
  "scripts": {
    "create-manifest": "node  -e \"$(cat lib/createManifest.js)\" --input-type=\"module\"",
    "dev": "NODE_OPTIONS='--inspect' yarn create-manifest && next dev --hostname 0.0.0.0",
    "build": "yarn create-manifest && next build",
    "start": "yarn create-manifest && next start"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "date-fns": "2.14.0",
    "html-react-parser": "^1.1.1",
    "next": "latest",
    "nprogress": "^0.2.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-slick": "^0.28.0",
    "react-swipeable": "^6.0.1",
    "react-transition-group": "^4.4.1",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "autoprefixer": "10.0.2",
    "dotenv": "^8.2.0",
    "oembed-parser": "^1.4.3",
    "postcss": "^8.1.7",
    "postcss-flexbugs-fixes": "4.2.1",
    "tailwindcss": "^2.0.1"
  },
  "license": "MIT"
}
